{
	"name": "veklabs.com",
	"repository": "@github:veklabs/veklabs.com",
	"private": true,
	"scripts": {
		"start": "run-p serve-hugo watch-webpack",
		"build": "run-s 'hugo --gc --minify' 'webpack --mode=production'",
		"watch-webpack": "webpack --mode=development --watch",
		"serve-hugo": "wait-on assets/css/main.css && hugo server -b http://$(hostname)/ --bind 0.0.0.0"
	},
	"dependencies": {
		"@babel/runtime": "^7.6.3",
		"@vimeo/player": "^2.10.0",
		"autoprefixer": "^9.6.1",
		"get-height": "^0.2.0",
		"netlify-lambda": "^1.5.0",
		"normalize.css": "^8.0.1",
		"popmotion": "^8.7.0",
		"postcss-autoreset": "^2.0.2",
		"postcss-cli": "^6.1.3",
		"postcss-color-hex-alpha": "^5.0.3",
		"postcss-custom-unit": "^1.0.0",
		"postcss-hexrgba": "^1.0.2",
		"postcss-import": "^12.0.1",
		"postcss-nesting": "^7.0.1",
		"postcss-partial-import": "^4.1.0",
		"postcss-short": "^5.0.0"
	},
	"browserslist": [
		"ie >= 10",
		"last 3 versions"
	],
	"devDependencies": {
		"@babel/core": "^7.6.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.6.2",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/preset-env": "^7.6.3",
		"@babel/preset-typescript": "^7.6.0",
		"babel-loader": "^8.0.6",
		"npm-run-all": "^4.1.5",
		"rollup": "^1.23.1",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"typescript": "^3.6.3",
		"wait-on": "^3.3.0",
		"webpack": "^4.41.0",
		"webpack-cli": "^3.3.9"
	}
}
