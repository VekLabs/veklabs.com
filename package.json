{
 "name": "veklabs.com",
 "repository": "@github:veklabs/veklabs.com",
 "private": true,
 "sideEffects": false,
 "scripts": {
  "start": "run-p serve-hugo watch-webpack",
  "build": "run-s build-webpack social-images build-hugo",
  "build:deploy-preview": "webpack --mode=production && yarn social-images && hugo --gc --minify --buildFuture -b $DEPLOY_PRIME_URL",
  "build:branch": "webpack --mode=production && yarn social-images && hugo --gc --minify -b $DEPLOY_PRIME_URL",
  "build:split": "webpack --mode=production && yarn social-images && hugo --gc --minify --enableGitInfo",
  "build-hugo": "hugo",
  "build-webpack": "webpack -p",
  "watch-webpack": "webpack --mode=development --watch",
  "social-images": "node scripts/social-image.mjs",
  "serve-hugo": "hugo server -b http://$(hostname) --bind 0.0.0.0",
  "forestry-hugo": "hugo server --port 8080 -D -E -F --renderToDisk -d public",
  "forestry": "run-p forestry-hugo watch-webpack"
 },
 "dependencies": {
  "@babel/core": "^7.9.0",
  "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
  "@babel/plugin-transform-runtime": "^7.9.0",
  "@babel/preset-env": "^7.9.5",
  "@babel/preset-typescript": "^7.9.0",
  "@babel/runtime": "^7.9.2",
  "@iarna/toml": "^2.2.5",
  "@juggle/resize-observer": "^3.1.3",
  "@rollup/plugin-babel": "^5.0.4",
  "@rollup/plugin-commonjs": "^13.0.0",
  "@rollup/plugin-multi-entry": "^3.0.1",
  "@rollup/plugin-node-resolve": "^8.1.0",
  "@types/autoprefixer": "^9.7.2",
  "@types/babel__core": "^7.1.7",
  "@types/postcss-import": "^12.0.0",
  "@types/rellax": "^1.7.2",
  "@types/vimeo__player": "^2.9.0",
  "@types/wait-on": "^4.0.0",
  "@types/webpack": "^4.41.11",
  "@vimeo/player": "^2.11.0",
  "babel-loader": "^8.1.0",
  "browserslist": "^4.14.2",
  "date-fns": "^2.14.0",
  "get-height": "^0.2.0",
  "gray-matter": "^4.0.2",
  "lodash": "^4.17.15",
  "markdown-it": "^11.0.0",
  "netlify-lambda": "^1.5.0",
  "npm-run-all": "^4.1.5",
  "popmotion": "^8.7.0",
  "postcss-cli": "^7.1.0",
  "postcss-normalize": "^9.0.0",
  "postcss-preset-env": "^6.7.0",
  "postcss-relaxed-unit": "^1.1.1",
  "puppeteer": "^5.3.0",
  "puppeteer-social-image": "^0.8.1",
  "ramjet": "^0.5.0",
  "rellax": "^1.12.1",
  "remark-frontmatter": "^2.0.0",
  "remark-parse": "^8.0.2",
  "remark-stringify": "^8.1.0",
  "rollup": "^2.21.0",
  "scroll-snap-api": "^1.0.1",
  "to-vfile": "^6.1.0",
  "typescript": "^3.8.3",
  "unified": "^9.0.0",
  "wait-on": "^4.0.2",
  "webpack": "^4.42.1",
  "webpack-cli": "^3.3.11"
 },
 "browserslist": [
  "ie >= 10",
  "last 3 versions"
 ]
}
